{
  "compilerOptions": {
    "target": "es2022",
    "useDefineForClassFields": false,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"paths": {
			"@components/*": [
				"stubber-pro-app/src/app/main/components/*"
			],
			"@services/*": [
				"stubber-pro-app/src/app/main/services/*"
			],
			"@models/*": [
				"stubber-pro-app/src/app/main/models/*"
			],
			"@views/*": [
				"stubber-pro-app/src/app/main/views/*"
			],
			"@core/*": [
				"stubber-pro-app/src/app/core/*"
			],
			"@root-store/*": [
				"stubber-pro-app/src/app/root-store/*"
			]
		}
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    },
    {
      "path": "./tsconfig.editor.json"
    }
  ],
  "extends": "../tsconfig.base.json",
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
